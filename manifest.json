{
  "manifest_version": 3,
  "name": "Tabman",
  "version": "1.0",
  "description": "Navigate through your tabs with ease",
  "icons": {
    "32": "icons/icon32.png",
    "64": "icons/icon64.png"
  },
  "action": {
    "default_icon": {
      "32": "icons/icon32.png",
    "64": "icons/icon64.png"
    },
    "default_popup": "popup.html"
  },
"permissions": ["tabs", "storage"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "Command+Shift+L"
      },
      "description": "Navigate through your tabs with ease"
    }
  }
}